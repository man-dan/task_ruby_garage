<div class="form">
   <div class="s">Sign up</div>
    <%= form_for(@user) do |f| %>
    <%= render 'shared/error_messages' %>
	 <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <p></p>
      <%= f.submit "Sign up", class: "btn btn-success" %>
    <% end %>
    <p>Have an account? <%= link_to "Sign in", signin_path %></p>

</div>
