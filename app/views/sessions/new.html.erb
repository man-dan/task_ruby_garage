<div class="form">
   <div class="s">Sign in</div>
    <%= form_for(:session, url: sessions_path) do |f| %>
	 <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <p></p>
      <%= f.submit "Sign in", class: "btn btn-success" %>
    <% end %>
    <p>New user? <%= link_to "Sign up now!", signup_path %></p>

</div>
